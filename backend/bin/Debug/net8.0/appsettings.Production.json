{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Secret": "${JWT_SECRET}",
    "Issuer": "SapBasisPulse",
    "Audience": "SapBasisPulseUsers",
    "ExpiresInMinutes": 60
  },
  "Auth": {
    "AutoActivateInDevelopment": "false",
    "BypassStatusCheckInDevelopment": "false",
    "ConsultantRegistrationEnabled": "false",
    "MessagingEnabled": "true"
  },
  "Authentication": {
    "Google": {
      "ClientId": "${GOOGLE_CLIENT_ID}",
      "ClientSecret": "${GOOGLE_CLIENT_SECRET}"
    }
  }
}